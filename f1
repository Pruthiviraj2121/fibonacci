package com.company;
import java.util.Scanner;

public class fibonacci{
    public static void main(String[] args) {
        int a=0,b=1,c;
        Scanner sc = new Scanner(System.in);
        System.out.println("enter a number::");
        int num = sc.nextInt();
        for(int i=0;i<=num;i++){
            c = a+b;
            System.out.println(c);
            a=b;
            b=c;
        }
    }

}
